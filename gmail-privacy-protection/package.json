{
  "name": "gmail-privacy-protection",
  "description": "Gmailのプライバシーを保護するための機能を提供します",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "clean": "rm -rf distributions",
    "npm:deps": "ncu",
    "npm:update-i": "ncu -i",
    "npm:update-all": "ncu -u",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier": "prettier --write \"**/*.{ts,js,css,html,md,json}\"",
    "tsc": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "ci": "npm-run-all prettier eslint:fix tsc test clean build"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/chrome": "^0.0.307",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.7",
    "@vitest/coverage-v8": "^3.0.7",
    "esbuild": "^0.25.0",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "lodash": "^4.17.21",
    "npm-check-updates": "^17.1.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.5.2",
    "type-fest": "^4.35.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0",
    "vitest": "^3.0.7"
  },
  "license": "MIT"
}
