{
  "name": "gmail-privacy-protection",
  "description": "Gmailのプライバシーを保護するための機能を提供します",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "clean": "rm -rf distributions",
    "package:deps": "ncu",
    "package:update": "ncu -i",
    "package:update:force": "ncu -u",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier": "prettier --check \"**/*.{ts,js,css,html,md,json}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,js,css,html,md,json}\"",
    "tsc": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "ci": "npm-run-all prettier:fix eslint:fix tsc test clean build"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/chrome": "^0.1.6",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "lodash": "^4.17.21",
    "npm-check-updates": "^18.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4"
  },
  "license": "MIT"
}
